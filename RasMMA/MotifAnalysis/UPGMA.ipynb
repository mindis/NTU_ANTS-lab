   },
   "outputs": [],
   "source": [
    "import scipy\n",
    "import scipy.cluster.hierarchy as sch\n",
    "\n",
    "def UPGMA(in_matrix):    \n",
    "    D = scipy.spatial.distance.squareform(in_matrix)\n",
    "    Z = sch.linkage(D, method='average')\n",
    "    return Z"
   ]
  }
